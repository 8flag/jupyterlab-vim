{
  "name": "jupyterlab_vim",
  "version": "0.1.1-dev",
  "description": "Code cell vim bindings",
  "author": "Jacques Kvam",
  "main": "lib/index.js",
  "keywords": [
    "jupyter",
    "jupyterlab"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "prepare": "npm run build",
    "watch": "tsc -w"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "jupyterlab": {
    "extension": true
  },
  "dependencies": {
    "@jupyterlab/application": "^0.13.0",
    "@jupyterlab/codemirror": "^0.13.0",
    "@jupyterlab/notebook": "^0.13.0",
    "@phosphor/commands": "^1.4.0",
    "@types/codemirror": "^0.0.53"
  },
  "devDependencies": {
    "rimraf": "^2.6.1",
    "typescript": "~2.4.0"
  }
}
